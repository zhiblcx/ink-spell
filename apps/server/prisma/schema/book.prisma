model Book {
  id          Int        @id @default(autoincrement())
  name        String?
  cover       String?
  protagonist String?
  description String?
  author      String?
  bookFile    String     @map("book_file")
  bookShelfId Int?       @map("book_shelf_id")
  BookShelf   BookShelf? @relation(fields: [bookShelfId], references: [id], onDelete: Cascade)
  md5         String
}
