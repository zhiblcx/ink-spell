{
  "framework": null,
  "outputDirectory": ".",
  "buildCommand": "pnpm run build",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api(.*)",
      "destination": "/api/restful"
    },
    {
      "source": "/(.*)",
      "destination": "/public"
    }
  ]
}
